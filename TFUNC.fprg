        <attribute name="edited" value="VXNlcjtSSUJLQUFBQTsyMDI1LTExLTExOzA5OjA4OjEyIFBNOzY7MjM2MQ=="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="N, hasil" type="Integer" array="False" size=""/>
            <input variable="N"/>
            <assign variable="hasil" expression="HitungRekursif(N)"/>
            <output expression="&quot;Bilangan yang dimasukkan : &quot;&amp;N" newline="True"/>
            <output expression="&quot;Hasil perhitungan rekursif : &quot; &amp; hasil" newline="True"/>
        </body>
    </function>
    <function name="HitungRekursif" type="Integer" variable="HasilRekursi">
        <parameters>
            <parameter name="angka" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="HasilRekursi" type="Integer" array="False" size=""/>
            <if expression="angka == 1">
                <then>
                    <assign variable="HasilRekursi" expression="1"/>
                </then>
                <else>
                    <assign variable="HasilRekursi" expression="angka + HitungRekursif(angka - 1)"/>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
